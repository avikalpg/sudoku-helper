{
	"name": "Sudoku Solver",
	"description": "Detect sudoku puzzle from screen and suggest next steps to solve the sudoku puzzle.",
	"version": "1.0",
	"manifest_version": 3,
	"background": {
		"service_worker": "background.js",
		"type": "module"
	},
	"permissions": [
		"storage",
		"activeTab",
		"scripting",
		"tabs"
	],
	"action": {
		"default_title": "Sudoku Solver",
		"default_popup": "popup.html",
		"default_icon": {
			"16": "/images/get_started16.png",
			"32": "/images/get_started32.png",
			"48": "/images/get_started48.png",
			"128": "/images/get_started128.png"
		}
	},
	"icons": {
		"16": "/images/get_started16.png",
		"32": "/images/get_started32.png",
		"48": "/images/get_started48.png",
		"128": "/images/get_started128.png"
	}
}